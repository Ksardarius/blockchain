{
  "name": "wallet-ui",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "wallet-web",
    "wallet-app"
  ],
  "scripts": {
    "build:wasm": "npm run --workspace wallet-web build",
    "build:wasm:watch": "npm run --workspace wallet-web build:watch",
    "start:ui": "npm run --workspace wallet-app start",
    "dev": "npm-run-all -p build:wasm:watch start:ui"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "wallet-web-wasm": "0.1.0"
  }
}
